# scripts/verify-manifest.js
cat > scripts/verify-manifest.js <<'JS'
const fs = require('fs'), crypto = require('crypto'), path = require('path');
function verifySync(manifest='package.json', sig='signatures/package.json.sig'){
  const pub = path.join(process.cwd(),'keys','rsa-4096-public.pem');
  if (!fs.existsSync(pub)) throw new Error('Public key missing: ' + pub);
  if (!fs.existsSync(manifest)) throw new Error('Manifest missing: ' + manifest);
  if (!fs.existsSync(sig)) throw new Error('Signature missing: ' + sig);
  const m = fs.readFileSync(manifest), s = fs.readFileSync(sig), p = fs.readFileSync(pub);
  const v = crypto.createVerify('RSA-SHA256'); v.update(m); return v.verify(p,s);
}
if (require.main === module) { try{ console.log('Verified:', verifySync(process.argv[2], process.argv[3])); process.exit(0);}catch(e){console.error(e.message); process.exit(2);} } else { module.exports = { verifySync }; }
